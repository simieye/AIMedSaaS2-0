
{
  "name": "ai_diagnosis",
  "title": "AI诊断记录",
  "description": "存储AI诊断分析结果，支持多科室专业化诊断",
  "schemas": [
    {
      "name": "diagnosis_id",
      "title": "诊断ID",
      "type": "String"
    },
    {
      "name": "patient_id",
      "title": "患者ID",
      "type": "String"
    },
    {
      "name": "doctor_id",
      "title": "医生ID",
      "type": "String"
    },
    {
      "name": "department",
      "title": "科室",
      "type": "String"
    },
    {
      "name": "symptoms",
      "title": "症状列表",
      "type": "Array"
    },
    {
      "name": "medical_data",
      "title": "医疗数据",
      "type": "Object"
    },
    {
      "name": "ai_model",
      "title": "AI模型信息",
      "type": "Object"
    },
    {
      "name": "diagnosis_result",
      "title": "诊断结果",
      "type": "Object"
    },
    {
      "name": "confidence_score",
      "title": "置信度",
      "type": "Number"
    },
    {
      "name": "recommendations",
      "title": "建议",
      "type": "Array"
    },
    {
      "name": "status",
      "title": "状态",
      "type": "String"
    },
    {
      "name": "created_at",
      "title": "创建时间",
      "type": "DateTime",
      "format": "timestamp-ms"
    }
  ]
}
