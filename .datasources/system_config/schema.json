      "type": "Array",
      "items": {
        "type": "String"
      }
    },
    {
      "name": "version",
      "title": "配置版本",
      "type": "String"
    },
    {
      "name": "updated_by",
      "title": "更新人",
      "type": "String"
    },
    {
      "name": "updated_at",
      "title": "更新时间",
      "type": "DateTime",
      "format": "timestamp-ms"
    },
    {
      "name": "created_by",
      "title": "创建人",
      "type": "String"
    },
    {
      "name": "created_at",
      "title": "创建时间",
      "type": "DateTime",
      "format": "timestamp-ms"
    },
    {
      "name": "change_history",
      "title": "变更历史",
      "type": "Array",
      "items": {
        "type": "Object",
        "schemas": [
          {
            "name": "version",
            "title": "版本",
            "type": "String"
          },
          {
            "name": "old_value",
            "title": "旧值",
            "type": "Object"
          },
          {
            "name": "new_value",
            "title": "新值",
            "type": "Object"
          },
          {
            "name": "changed_by",
            "title": "修改人",
            "type": "String"
          },
          {
            "name": "changed_at",
            "title": "修改时间",
            "type": "DateTime",
            "format": "timestamp-ms"
          },
          {
            "name": "change_reason",
            "title": "修改原因",
            "type": "String"
          }
        ]
      }
