
{
  "name": "system_config",
  "title": "系统配置",
  "schemas": [
    {
      "name": "config_id",
      "title": "配置ID",
      "type": "String"
    },
    {
      "name": "config_key",
      "title": "配置键",
      "type": "String"
    },
    {
      "name": "config_value",
      "title": "配置值",
      "type": "Object"
    },
    {
      "name": "value_type",
      "title": "值类型",
      "type": "String"
    },
    {
