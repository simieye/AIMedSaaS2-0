
{
  "name": "agent_experiments",
  "title": "智能体训练实验",
  "description": "存储AI智能体训练实验的完整生命周期数据，包括实验配置、训练过程、结果评估和PCCP审计记录，符合医疗AI监管要求",
  "schemas": [
    {
      "name": "experiment_id",
      "title": "实验ID",
      "type": "String",
      "description": "实验唯一标识符"
    },
    {
      "name": "agent_id",
      "title": "智能体ID",
      "type": "String",
      "description": "关联的智能体标识符"
    },
    {
      "name": "dataset_id",
      "title": "数据集ID",
      "type": "String",
      "description": "使用的训练数据集标识符"
    },
    {
      "name": "experiment_name",
      "title": "实验名称",
      "type": "String"
    },
    {
      "name": "description",
      "title": "实验描述",
      "type": "MultiLineString"
    },
    {
      "name": "config",
      "title": "实验配置",
      "type": "Object",
      "description": "完整的实验配置参数",
      "schemas": [
        {
          "name": "model_config",
          "title": "模型配置",
          "type": "Object",
          "schemas": [
            {
              "name": "model_type",
              "title": "模型类型",
              "type": "String"
            },
            {
              "name": "model_version",
              "title": "模型版本",
              "type": "String"
            },
            {
              "name": "architecture",
              "title": "架构",
              "type": "String"
            },
            {
              "name": "pretrained_model",
              "title": "预训练模型",
              "type": "String"
            }
          ]
        },
        {
          "name": "training_config",
          "title": "训练配置",
          "type": "Object",
          "schemas": [
            {
              "name": "epochs",
              "title": "训练轮数",
              "type": "Number"
            },
            {
              "name": "batch_size",
              "title": "批次大小",
              "type": "Number"
            },
            {
              "name": "learning_rate",
              "title": "学习率",
              "type": "Number"
            },
            {
              "name": "optimizer",
              "title": "优化器",
              "type": "String"
            },
            {
              "name": "loss_function",
              "title": "损失函数",
              "type": "String"
            },
            {
              "name": "validation_split",
              "title": "验证集比例",
              "type": "Number"
            }
          ]
        },
        {
          "name": "data_config",
          "title": "数据配置",
          "type": "Object",
          "schemas": [
            {
              "name": "data_augmentation",
              "title": "数据增强",
              "type": "Boolean"
            },
            {
              "name": "normalization",
              "title": "归一化",
              "type": "Boolean"
            },
            {
              "name": "validation_strategy",
              "title": "验证策略",
              "type": "String"
            }
          ]
        },
        {
          "name": "hyperparameters",
          "title": "超参数",
          "type": "Object"
        },
        {
          "name": "environment",
          "title": "运行环境",
          "type": "String"
        }
      ]
    },
    {
      "name": "metrics",
      "title": "评估指标",
      "type": "Object",
      "description": "训练过程和结果的评估指标",
      "schemas": [
        {
          "name": "training_metrics",
          "title": "训练指标",
          "type": "Object",
          "schemas": [
            {
              "name": "loss",
              "title": "损失值",
              "type": "Object",
              "schemas": [
                {
                  "name": "training_loss",
                  "title": "训练损失",
                  "type": "Number"
                },
                {
                  "name": "validation_loss",
                  "title": "验证损失",
                  "type": "Number"
                },
                {
                  "name": "test_loss",
                  "title": "测试损失",
                  "type": "Number"
                }
              ]
            },
            {
              "name": "accuracy",
              "title": "准确率",
              "type": "Object",
              "schemas": [
                {
                  "name": "training_accuracy",
                  "title": "训练准确率",
                  "type": "Number"
                },
                {
                  "name": "validation_accuracy",
                  "title": "验证准确率",
                  "type": "Number"
                },
                {
                  "name": "test_accuracy",
                  "title": "测试准确率",
                  "type": "Number"
                }
              ]
            },
            {
              "name": "additional_metrics",
              "title": "其他指标",
              "type": "Object",
              "schemas": [
                {
                  "name": "precision",
                  "title": "精确率",
                  "type": "Number"
                },
                {
                  "name": "recall",
                  "title": "召回率",
                  "type": "Number"
                },
                {
                  "name": "f1_score",
                  "title": "F1分数",
                  "type": "Number"
                },
                {
                  "name": "auc_roc",
                  "title": "AUC-ROC",
                  "type": "Number"
                },
                {
                  "name": "confusion_matrix",
                  "title": "混淆矩阵",
                  "type": "Array",
                  "items": {
                    "type": "Array"
                  }
                }
              ]
            }
          ]
        },
        {
          "name": "performance_metrics",
          "title": "性能指标",
          "type": "Object",
          "schemas": [
            {
              "name": "inference_time",
              "title": "推理时间",
              "type": "Number"
            },
            {
              "name": "memory_usage",
              "title": "内存使用",
              "type": "Number"
            },
            {
              "name": "model_size",
              "title": "模型大小",
              "type": "Number"
            },
            {
              "name": "throughput",
              "title": "吞吐量",
              "type": "Number"
            }
          ]
        },
        {
          "name": "business_metrics",
          "title": "业务指标",
          "type": "Object",
          "schemas": [
            {
              "name": "cost",
              "title": "成本",
              "type": "Number"
            },
            {
              "name": "efficiency",
              "title": "效率",
              "type": "Number"
            },
            {
              "name": "user_satisfaction",
              "title": "用户满意度",
              "type": "Number"
            }
          ]
        }
      ]
    },
    {
      "name": "status",
      "title": "实验状态",
      "type": "String",
      "description": "pending|running|completed|failed|paused|cancelled"
    },
    {
      "name": "progress",
      "title": "训练进度",
      "type": "Object",
      "schemas": [
        {
          "name": "current_epoch",
          "title": "当前轮次",
          "type": "Number"
        },
        {
          "name": "total_epochs",
          "title": "总轮次",
          "type": "Number"
        },
        {
          "name": "progress_percentage",
          "title": "进度百分比",
          "type": "Number"
        },
        {
          "name": "estimated_completion",
          "title": "预计完成时间",
          "type": "DateTime",
          "format": "timestamp-ms"
        },
        {
          "name": "start_time",
          "title": "开始时间",
          "type": "DateTime",
          "format": "timestamp-ms"
        },
        {
          "name": "end_time",
          "title": "结束时间",
          "type": "DateTime",
          "format": "timestamp-ms"
        }
      ]
    },
    {
      "name": "created_by",
      "title": "创建者",
      "type": "String",
      "description": "实验创建者用户ID"
    },
    {
      "name": "pccp_log_id",
      "title": "PCCP日志ID",
      "type": "String",
      "description": "关联的PCCP合规日志记录"
    },
    {
      "name": "published_version",
      "title": "发布版本",
      "type": "String",
      "description": "实验成功后的发布版本号"
    },
    {
      "name": "experiment_type",
      "title": "实验类型",
      "type": "String",
      "description": "training|fine_tuning|evaluation|ab_test"
    },
    {
      "name": "priority",
      "title": "优先级",
      "type": "String",
      "description": "low|medium|high|critical"
    },
    {
      "name": "tags",
      "title": "标签",
      "type": "Array",
      "items": {
        "type": "String"
      }
    },
    {
      "name": "notes",
      "title": "实验备注",
      "type": "MultiLineString"
    },
    {
      "name": "artifacts",
      "title": "实验产物",
      "type": "Object",
      "schemas": [
        {
          "name": "model_files",
          "title": "模型文件",
          "type": "Array",
          "items": {
            "type": "String"
          }
        },
        {
          "name": "logs",
          "title": "日志文件",
          "type": "Array",
          "items": {
            "type": "String"
          }
        },
        {
          "name": "checkpoints",
          "title": "检查点",
          "type": "Array",
          "items": {
            "type": "String"
          }
        },
        {
          "name": "reports",
          "title": "报告文件",
          "type": "Array",
          "items": {
            "type": "String"
          }
        }
      ]
    },
    {
      "name": "compliance",
      "title": "合规信息",
      "type": "Object",
      "description": "PCCP合规相关记录",
      "schemas": [
        {
          "name": "approval_status",
          "title": "审批状态",
          "type": "String"
        },
        {
          "name": "risk_level",
          "title": "风险等级",
          "type": "String"
        },
        {
          "name": "reviewer",
          "title": "审核人",
          "type": "String"
        },
        {
          "name": "review_date",
          "title": "审核日期",
            "type": "DateTime",
            "format": "timestamp-ms"
        },
        {
          "name": "compliance_score",
          "title": "合规评分",
          "type": "Number"
        }
      ]
    },
    {
      "name": "resource_usage",
      "title": "资源使用",
      "type": "Object",
      "schemas": [
        {
          "name": "gpu_hours",
          "title": "GPU小时数",
          "type": "Number"
        },
        {
          "name": "cpu_hours",
          "title": "CPU小时数",
          "type": "Number"
        },
        {
          "name": "memory_gb",
          "title": "内存使用GB",
          "type": "Number"
        },
        {
          "name": "storage_gb",
          "title": "存储使用GB",
          "type": "Number"
        }
      ]
    }
  ]
}
